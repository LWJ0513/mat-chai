
type User {
    email: String
    username: String
    firstName: String
    lastName: String
    dateOfBirth: String
    age: Int

    gender: String
    sexualPreferences: [String]
    biography: String
    interests: [String]
    profilePictureUrl: String
    pictureUrls: [String]
    fame: Float
}


type Query {
    getUserByUsername(username: String!): User
}
